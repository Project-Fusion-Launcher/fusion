{
  "productName": "fusion",
  "version": "0.0.0",
  "identifier": "fusion",
  "build": {
    "beforeDevCommand": "vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "tsc && vite build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Fusion",
        "width": 800,
        "minWidth": 760,
        "height": 600,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
